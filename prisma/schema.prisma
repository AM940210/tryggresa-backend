datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Trip {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  date        String
  time        String
  fromAddress String?
  toAddress   String?
  people      Int
  wheelchair  Boolean
}
