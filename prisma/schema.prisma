generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Trip {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  from        String
  to          String
  date        String
  time        String
  passengers  Int
  wheelchair  Boolean
}
